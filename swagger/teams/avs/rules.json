{
  "teamName": "avs",
  "baseUrl": "https://avs.scff.stg.chewy.com",
  
  "endpoints": {
    "/avs/v1.0/verifyAddress": {
      "method": "POST",
      "description": "Verify a single address",
      "responseType": "object"
    },
    "/avs/v1.0/suggestAddresses": {
      "method": "POST", 
      "description": "Get address suggestions",
      "responseType": "array"
    }
  },

  "responseCodeBehaviors": {
    "VERIFIED": {
      "description": "Address is valid and complete - no corrections needed",
      "validatedAddress": "populated",
      "requestAddressSanitized": "null",
      "triggers": ["valid complete address", "all fields correct"]
    },
    "CORRECTED": {
      "description": "Address is valid but some fields were corrected or added",
      "validatedAddress": "populated",
      "requestAddressSanitized": "null",
      "triggers": ["empty postal code", "incorrect city", "wrong state", "missing state and postal"]
    },
    "STREET_PARTIAL": {
      "description": "Only the street name is verified, not the full premises",
      "validatedAddress": "null",
      "requestAddressSanitized": "populated",
      "triggers": ["wrong street number", "street name only without number"]
    },
    "PREMISES_PARTIAL": {
      "description": "Street address is partially verified",
      "validatedAddress": "populated",
      "requestAddressSanitized": "null",
      "triggers": ["partial premises match"]
    },
    "NOT_VERIFIED": {
      "description": "Address cannot be verified - invalid or non-existent",
      "validatedAddress": "null",
      "requestAddressSanitized": "populated",
      "triggers": ["mismatched city/state/postal", "non-existent address", "fake address"]
    }
  },

  "testData": {
    "workingAddresses": {
      "verifyAddress": {
        "streets": ["600 HARLAN CT"],
        "city": "Bonaire",
        "stateOrProvince": "GA",
        "postalCode": "31005-5427",
        "country": "US"
      },
      "suggestAddresses": {
        "streets": ["1600 Amphitheatre Pkwy"],
        "city": "Mountain View",
        "stateOrProvince": "CA",
        "country": "US"
      }
    },
    "requiredFields": {
      "/avs/v1.0/verifyAddress": ["streets", "city", "stateOrProvince", "postalCode", "country"],
      "/avs/v1.0/suggestAddresses": ["streets", "city", "country"]
    }
  },

  "assertionPatterns": {
    "responseCodeProperty": "responseCode",
    "responseCodeValues": ["VERIFIED", "CORRECTED", "STREET_PARTIAL", "PREMISES_PARTIAL", "NOT_VERIFIED"]
  },

  "featureFileGeneration": {
    "dataTableFields": ["streets", "city", "stateOrProvince", "postalCode", "country"],
    "defaultEndpoint": "/avs/v1.0/verifyAddress"
  }
}
